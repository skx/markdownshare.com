</ul>
</div>
<!-- /tmpl_if -->
<!-- tmpl_if name='html' -->
    <blockquote>
      <!-- tmpl_var name='html' -->
      <p style="text-align: right;"><a href="/raw/<!-- tmpl_var name='id' -->">View the RAW markdown</a>.</p>
    </blockquote>
<!-- tmpl_else -->
    <h1>Not Found</h1>
    <blockquote>
      <p>The rendered Markdown you were looking for is not here.</p>
    </blockquote>
<!-- /tmpl_if -->
</div>
<div class="clear"> </div>
<!-- tmpl_include name='footer.inc' -->
</div>
